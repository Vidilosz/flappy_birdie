<Window x:Class="flappy_birdie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:flappy_birdie"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

	<Canvas>
		<Canvas.Background>
			<ImageBrush ImageSource="/Images/pc2.jpg"/>
		</Canvas.Background>

		<Image Source="/Images/floppy2.jpg" x:Name="madar" Canvas.Left="165" Canvas.Top="96" HorizontalAlignment="Left" VerticalAlignment="Top" Width="23" Height="24"/>

		<Button x:Name="Start" Content="Start" Canvas.Left="317" Canvas.Top="44" Height="65" Width="164" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="35" Background="{x:Null}" Click="Start_Click" BorderBrush="{x:Null}"/>
		<Button x:Name="Normal" Content="Normal" Canvas.Left="323" Canvas.Top="22" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="35" Background="{x:Null}" Click="Normal_Click" BorderBrush="{x:Null}"/>
		<Button x:Name="Rainy_day" Content="Rainy day" Canvas.Left="323" Canvas.Top="98" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="35" Background="{x:Null}" Click="Rainy_day_Click" BorderBrush="{x:Null}"/>
		<Button x:Name="TFI" Content="ThE foG IS COmIng" Canvas.Left="322" Canvas.Top="168" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="19" Background="{x:Null}" Click="TFI_Click" BorderBrush="{x:Null}"/>

		<Rectangle Height="217" Canvas.Left="40" Stroke="Black" Canvas.Top="220" Width="724" Fill="#FFF9EECB" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Ellipse Height="50" Canvas.Left="485" Stroke="Red" Canvas.Top="330" Width="50" Fill="Red"/>
		<Ellipse Height="50" Canvas.Left="563" Stroke="Red" Canvas.Top="283" Width="50" Fill="Red" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Rectangle Height="29" Stroke="Black" Width="20" Canvas.Left="229" Canvas.Top="296" HorizontalAlignment="Left" VerticalAlignment="Top" Fill="Black"/>
		<Rectangle Height="29" Stroke="Black" Width="20" Canvas.Left="229" Canvas.Top="351" Fill="Black" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Rectangle Height="29" Stroke="Black" Width="20" Canvas.Left="202" Canvas.Top="323" Fill="Black" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
			<Rectangle.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform Angle="91.096"/>
					<TranslateTransform/>
				</TransformGroup>
			</Rectangle.RenderTransform>
		</Rectangle>
		<Rectangle Height="29" Stroke="Black" Width="20" Canvas.Left="259" Canvas.Top="323" Fill="Black" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
			<Rectangle.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform Angle="91.134"/>
					<TranslateTransform/>
				</TransformGroup>
			</Rectangle.RenderTransform>
		</Rectangle>
		<Ellipse Height="50" Canvas.Left="215" Stroke="Black" Canvas.Top="314" Width="50" Fill="Black"/>
		<Ellipse Height="10" Canvas.Left="271" Stroke="Black" Canvas.Top="333" Width="10" RenderTransformOrigin="0.475,0.531" Fill="White"/>
		<Ellipse Height="10" Canvas.Left="235" Stroke="Black" Canvas.Top="300" Width="10" RenderTransformOrigin="0.475,0.531" Fill="White" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Ellipse Height="10" Canvas.Left="234" Stroke="Black" Canvas.Top="367" Width="10" RenderTransformOrigin="0.475,0.531" Fill="White" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Ellipse Height="10" Canvas.Left="201" Stroke="Black" Canvas.Top="333" Width="10" RenderTransformOrigin="0.475,0.531" Fill="White" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Ellipse Height="34" Canvas.Left="223" Stroke="Black" Canvas.Top="321" Width="34" Fill="#FF5F5353" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Button x:Name="jump_2" Content="B" Canvas.Left="551" Canvas.Top="275" Background="{x:Null}" BorderBrush="{x:Null}" Height="64" Width="73" FontWeight="Bold" FontSize="30" HorizontalAlignment="Left" VerticalAlignment="Top" Click="jump_2_Click"/>
		<Button x:Name="jump_1" Content="A" Canvas.Left="474" Canvas.Top="321" Background="{x:Null}" BorderBrush="{x:Null}" Height="64" Width="73" FontWeight="Bold" FontSize="30" HorizontalAlignment="Left" VerticalAlignment="Top" Click="jump_1_Click_1"/>

	</Canvas>
</Window>
