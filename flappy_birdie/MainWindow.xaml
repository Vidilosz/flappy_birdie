<Window x:Class="flappy_birdie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:flappy_birdie"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

	<Canvas>
		<Ellipse x:Name="madar" Height="50" Canvas.Left="55" Canvas.Top="199" Width="50" Opacity="0.99" Fill="#FFEDC229" HorizontalAlignment="Left" VerticalAlignment="Center"/>
		<Button x:Name="Start" Content="Start" Canvas.Left="317" Canvas.Top="65" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="35" Background="#FFEFEFEF" Click="Start_Click"/>
		<Button x:Name="Normal" Content="Normal" Canvas.Left="317" Canvas.Top="130" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="35" Background="#FFEFEFEF" Click="Normal_Click"/>
		<Button x:Name="Rainy_day" Content="Rainy day" Canvas.Left="317" Canvas.Top="230" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="35" Background="#FFEFEFEF" Click="Rainy_day_Click"/>
		<Button x:Name="TFI" Content="ThE foG IS COmIng" Canvas.Left="317" Canvas.Top="330" Height="65" Width="164" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="19" Background="#FFC1BDBD" Click="TFI_Click"/>

	</Canvas>
</Window>
